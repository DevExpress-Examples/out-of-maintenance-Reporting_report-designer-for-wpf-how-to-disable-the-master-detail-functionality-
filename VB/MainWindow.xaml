<Window x:Class="DisableMasterDetailWizard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DisableMasterDetailWizard"
        mc:Ignorable="d"
        Title="MainWindow" SizeToContent="WidthAndHeight">
    <StackPanel>
        <Button Content="Use WizardCustomizationService" Click="UseWizardCustomizationService" Padding="12,4" Margin="12" />
        <Button Content="Use ServicesRegistry" Click="UseServicesRegistry" Padding="12,4" Margin="12" />
    </StackPanel>
</Window>
